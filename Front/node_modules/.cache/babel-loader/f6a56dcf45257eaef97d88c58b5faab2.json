ources":["value.vue"],"sourcesContent":["<!--货币对当前汇率和历史汇率数据\r\n `stk_id` int NOT NULL AUTO_INCREMENT COMMENT '自增序号',\r\n  `stk_no` varchar(50) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '货币编号SD CAD EUR',\r\n  `stk_name` varchar(50) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '货币中文简称美元 加元 欧元',\r\n  `stk_price` double DEFAULT NULL COMMENT '当前汇率价格与人民币的汇率',\r\n  `min_perc` int DEFAULT NULL COMMENT '委托报价下限%',\r\n  `max_perc` int DEFAULT NULL COMMENT '委托报价上限%',\r\n  `min_qty` int DEFAULT NULL COMMENT '最小委托数量',\r\n  `max_qty` int DEFAULT NULL COMMENT '最大委托数量',\r\n  `stk_date` varchar(50) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '汇率价格日期',\r\n  `stk_time` varchar(50) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '汇率价格时间',\r\n  `stk_unit` int DEFAULT NULL COMMENT '汇率单位日元为100，其它为1',\r\n  `stk_status` varchar(50) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '状态，日元为100，其它为1',\r\n  `stk_src` varchar(50) COLLATE utf8mb4_general_ci DEFAULT NULL COMMENT '价格来源',\r\n  PRIMARY KEY (`stk_id`)\r\n-->\r\n\r\n<template>\r\n  <div>\r\n    <!-- 可用资金 -->\r\n    <el-row>\r\n      <el-col :span=\"5\"> 可用资金: {{ balance }} </el-col>\r\n    </el-row>\r\n    <!-- 持仓列表 表格 -->\r\n    <el-table\r\n        align=\"center\"\r\n        :data=\"\r\n        tableData.slice(\r\n          (query.currentPage - 1) * query.pageSize,\r\n          query.currentPage * query.pageSize\r\n        )\r\n      \"\r\n        border\r\n        style=\"padding-top: 20px;\"\r\n        @sort-change=\"changeTableSort\"\r\n    >\r\n      <el-table-column\r\n          prop=\"code\"\r\n          label=\"代码\"\r\n          align=\"center\"\r\n          sortable\r\n          :sort-orders=\"['ascending', 'descending']\"\r\n      />\r\n      <el-table-column prop=\"name\" label=\"名称\" align=\"center\"/>\r\n      <el-table-column prop=\"count\" label=\"股票数量\"   align=\"center\" />\r\n      <el-table-column prop=\"cost\" label=\"总投入\"   align=\"center\"/>\r\n      <el-table-column label=\"成本\" />\r\n    </el-table>\r\n    <!-- 分页空间 刷新按钮 -->\r\n    <div class=\"pagination\">\r\n      <el-button\r\n          round\r\n          type=\"primary\"\r\n          size=\"mini\"\r\n          style=\"margin-top: 2px; float: right\"\r\n          icon=\"el-ihexo con-refresh\"\r\n      >\r\n        刷新\r\n      </el-button>\r\n      <el-pagination\r\n          layout=\"total, prev, pager, next\"\r\n          :current-page=\"query.currentPage\"\r\n          :page-size=\"query.pageSize\"\r\n          :total=\"dataTotalCount\"\r\n          background\r\n          @current-change=\"handlePageChange\"\r\n      />\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      balance: 10,\r\n      tableData: [\r\n        { code: \"600025\", name: \"华能水电\", count: 100, cost: 20 },\r\n        { code: \"600000\", name: \"浦发银行\", count: 100, cost: 20 },\r\n        { code: \"000001\", name: \"平安银行\", count: 100, cost: 20 },\r\n        { code: \"600886\", name: \"国投电力\", count: 100, cost: 20 },\r\n      ],\r\n      // 总记录数\r\n      dataTotalCount: 4,\r\n      query: {\r\n        // 当前页码\r\n        currentPage: 1,\r\n        // 每页显示的记录\r\n        pageSize: 2,\r\n      },\r\n    };\r\n  },\r\n\r\n    handlePageChange(val) {\r\n      this.$set(this.query, \"currentPage\", val);\r\n    },\r\n\r\n    changeTableSort(column) {\r\n      if (column.order == \"descending\") {\r\n        this.tableData = this.tableData.sort();\r\n      } else {\r\n        this.tableData = this.tableData.sort(\r\n            (a, b) => a[column.prop] - b[column.prop]\r\n        );\r\n      }\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"]}]}