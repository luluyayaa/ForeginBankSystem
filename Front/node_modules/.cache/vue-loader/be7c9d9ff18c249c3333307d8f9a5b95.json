e>29311110\r\n      </el-descriptions-item>\r\n      <el-descriptions-item>\r\n        <template slot=\"label\" >\r\n          <i class=\"el-icon-user\"></i>姓名\r\n        </template>lulu\r\n      </el-descriptions-item>\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          <i class=\"el-icon-mobile-phone\"></i>保证金\r\n        </template>\r\n        181000000\r\n      </el-descriptions-item>\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          <i class=\"el-icon-location-outline\"></i>仓位\r\n        </template>-57.3214\r\n      </el-descriptions-item>\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          <i class=\"el-icon-tickets\"></i>成本\r\n        </template>\r\n        <el-tag size=\"small\">-22412.1</el-tag>\r\n      </el-descriptions-item>\r\n      <el-descriptions-item>\r\n        <template slot=\"label\">\r\n          <i class=\"el-icon-office-building\"></i>市值\r\n        </template> 1332.42\r\n      </el-descriptions-item>\r\n    </el-descriptions>\r\n\r\n    <!-- 持仓详细信息表 AccountStk-->\r\n    <div style=\"margin:40px;padding-left: 42%\"><i class=\"el-icon-user\"/> 账户持仓详细信息</div>\r\n    <el-table  :data=\"tableData\"  show-summary stripe  style=\"width: 100%\" :row-class-name=\"tableRowClassName\">\r\n      <el-table-column prop=\"userNo\" label=\"名称\" align=\"center\" />\r\n      <el-table-column prop=\"stkNo\" label=\"编号\" align=\"center\" />\r\n      <el-table-column prop=\"stkCost\" label=\"成本\" align=\"center\" />\r\n      <el-table-column prop=\"stkQty\" label=\"市值\" align=\"center\" />\r\n      <el-table-column prop=\"balance\" label=\" 小计\" align=\"center\" />\r\n    </el-table>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {getAllAccountStk} from \"@/api/data\";\r\n\r\nexport default {\r\n  methods: {\r\n    tableRowClassName(rowIndex) {\r\n      if (rowIndex === -1) {\r\n        return 'warning-row';\r\n      }\r\n    },\r\n    getSummaries(param) {\r\n      const { columns, data } = param;\r\n      const sums = [];\r\n      columns.forEach((column, index) => {\r\n        if (index === 0) {\r\n          sums[index] = '总价';\r\n          return;\r\n        }\r\n        const values = data.map(item => Number(item[column.property]));\r\n        if (!values.every(value => isNaN(value))) {\r\n          sums[index] = values.reduce((prev, curr) => {\r\n            const value = Number(curr);\r\n            if (!isNaN(value)) {\r\n              return prev + curr;\r\n            } else {\r\n              return prev;\r\n            }\r\n          }, 0);\r\n          sums[index] += ' 元';\r\n        } else {\r\n          sums[index] = 'N/A';\r\n        }\r\n      });\r\n\r\n      return sums;\r\n    }\r\n  },\r\n  data(){\r\n    return{\r\n      tableData:[],\r\n    }\r\n  },\r\n  //钩子函数，VUE对象初始化完成后自动执行\r\n  mounted() {\r\n    getAllAccountStk().then((res)=>{\r\n      this.tableData= res.data.data;\r\n    });\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.el-table .warning-row {\r\n  background: oldlace;\r\n}\r\n.el-table .success-row {\r\n  background: #f0f9eb;\r\n}\r\n</style>\r\n"]}]}